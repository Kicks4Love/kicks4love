<h1>Edit Feature Post</h1>

<hr/><br/>

<%= form_for @feature_post, :url => admin_feature_post_path(@feature_post), :method => :PUT do |f| %>
	<%= render :partial => "form", :locals => {:form => f} %>
<% end %>

<br/>

<%= link_to "Back", admin_feature_posts_path, :class => "btn btn-default" %> 

<% content_for :end_of_body do %>
    <%= javascript_include_tag "admin/features.js" %>
<% end %>