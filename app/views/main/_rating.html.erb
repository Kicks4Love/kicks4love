<%= hidden_field_tag "page_source", @post_data[:post_type] %>
<%= hidden_field_tag "post_id", @post_data[:post_id] %>
<div class="rating">
	<% (1..@post_data[:post_rate]).each do |i| %>
    	<%= image_tag "sneakerblack.png", :height => "15" %>
    <% end %>
    <% (@post_data[:post_rate]+1..5).each do |i| %>
    	<%= image_tag "sneakergray.png", :height => "15" %>
    <% end %>
</div>