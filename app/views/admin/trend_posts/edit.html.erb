<h1>Edit Trend Post</h1>

<hr/><br/>

<%= form_for @trend_post, :url => admin_trend_post_path(@trend_post), :method => :PUT do |f| %>
	<%= render :partial => "form", :locals => {:form => f} %>
<% end %>

<br/>

<%= link_to "Back", admin_trend_posts_path, :class => "btn btn-default" %>

<% content_for :end_of_body do %>
    <%= javascript_include_tag "admin/trend.js" %>
<% end %>