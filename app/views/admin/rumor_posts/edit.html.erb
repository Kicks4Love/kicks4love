<h1>Edit Rumor Post</h1>

<hr/><br/>

<%= form_for @rumor_post, :url => admin_rumor_post_path(@rumor_post), :method => :PUT do |f| %>
	<%= render :partial => "form", :locals => {:form => f} %>
<% end %>

<br/>

<%= link_to "Back", admin_rumor_posts_path, :class => "btn btn-default" %>

<% content_for :end_of_body do %>
    <%= javascript_include_tag "admin/rumors.js" %>
<% end %>