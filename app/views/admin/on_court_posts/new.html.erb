<% content_for :head do %>
    <link rel="stylesheet" href="https://code.jquery.com/ui/1.12.1/themes/base/jquery-ui.css">
<% end %>

<h1>New On Court Post</h1>

<hr/><br/>

<%= form_for @on_court_post, :url => admin_on_court_posts_path do |f| %>
	<%= render :partial => "oncourtform", :locals => {:form => f} %>
<% end %>

<br/>

<%= link_to "Back", admin_on_court_posts_path, :class => "btn btn-default" %>

<% content_for :end_of_body do %>
    <script src="https://code.jquery.com/ui/1.10.4/jquery-ui.js"></script>
    <%= javascript_include_tag "admin/oncourt.js" %>
<% end %>